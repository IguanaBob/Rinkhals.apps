{
    "$version": "1",

    "name": "NUT-Client",
    "description": "Client for Network UPS Tools (NUT) to monitor UPS device connected to the printer",
    "version": "0.1",
    "depends": [],

    "requirements":
    {
        "cpu": 5,
        "memory": 10
    },

    "properties": {
        "address": {
            "display": "NUT server IP address or hostname",
            "type": "text"
        },
        "number_input": {
            "display": "Port",
            "type": "number",
            "range": [ 1024, 65536 ],
            "default": 3493
        },
        "user": {
            "display": "Username",
            "type": "text"
        },
        "password": {
            "display": "Password",
            "type": "text"
        },
        "ups_name": {
            "display": "UPS name",
            "type": "text"
        },
        "text_output": {
            "display": "Status",
            "type": "report"
        }
    }
}
